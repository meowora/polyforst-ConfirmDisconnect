{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${version}",
	"name": "${name}",
	"description": "${description}",
	"authors": [
		"Microcontrollers"
	],
	"contributors": [
      "Tellinq",
      "Meoworawr"
	],
	"contact": {
		"homepage": "${modrinth}",
		"sources": "${source}",
		"issues": "${issues}"
	},
	"license": "${license}",
	"icon": "assets/${mod_id}/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"dev.microcontrollers.confirmdisconnect.ConfirmDisconnect"
		]
	},
	"mixins": [
		"mixins.confirmdisconnect.json"
	],
	"depends": {
		"fabricloader": ">=0.16.7",
		"minecraft": "${mcdep}",
		"fabric-api": "*"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.kofi": "${kofi}",
				"modmenu.discord": "${discord}",
				"modmenu.modrinth": "${modrinth}",
				"modmenu.curseforge": "${curseforge}"
			},
			"update_checker": true
		}
	}
}
